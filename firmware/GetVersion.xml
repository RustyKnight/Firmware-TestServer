<cioffi type="function" name="GetVersion">
	<request>
		<header>
			<type>1</type>
			<version>1</version>
		</header>
	</request>
	<response>
		<header>
			<type>2</type>
			<version>1</version>
		</header>
		<group name="firmware">
			<property name="majorVersion" variable="majorVersion" type="Int" default="1"/>
			<property name="minorVersion" variable="minorVersion" type="Int" default="1"/>
			<property name="patchVersion" variable="patchVersion" type="Int" default="1"/>
		</group>
	</response>	
</cioffi>