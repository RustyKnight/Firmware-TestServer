<cioffi type="function" name="GetBatteryStatusChangeNotification">
	<response>
		<header>
			<type>24</type>
			<version>1</version>
		</header>
		<group name="battery">
			<property name="charge" variable="batteryCharge" type="Int" default="0">
				<allowed type="range" from="0" to="100"/>
			</property>
			<property name="status" variable="batteryStatus" type="Int" default="0">
				<allowed type="value" name="Unknown" value="0"/>
				<allowed type="value" name="Charging" value="1"/>
				<allowed type="value" name="Discharging" value="2"/>
				<allowed type="value" name="Not Charging" value="3"/>
				<allowed type="value" name="Full" value="4"/>
			</property>
			<property name="present" variable="batteryPresent" type="Bool" default="true"/>
			<property name="voltage" variable="batteryVoltage" type="Double" default="0">
				<allowed type="range" from="0" to="12"/>
			</property>
		</group>
	</response>	
</cioffi>