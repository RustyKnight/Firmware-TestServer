<cioffi type="notification" name="NetworkRegistrationStatusNotification">
	<response>
		<header>
			<type>16</type>
			<version>1</version>
		</header>
		<group name="connection">
			<property name="module" variable="module" type="Int" default="0">
				<allowed name="Satellite" value="0"/>
				<allowed name="Cellular" value="1"/>
			</property>
		</group>
		<group name="registration">
			<property name="status" variable="status" type="Int" default="0">
				<allowed name="Unknown" value="0"/>
				<allowed name="Registering" value="1"/>
				<allowed name="Registered Home Network" value="2"/>
				<allowed name="Registered Roaming" value="3"/>
				<allowed name="Registration Denied" value="4"/>
			</property>
		</group>
	</response>	
</cioffi>