<cioffi type="function" name="GetNetworkRegistration">
	<request>
		<header>
			<type>14</type>
			<version>1</version>
		</header>
	</request>
	<response>
		<header>
			<type>15</type>
			<version>1</version>
		</header>
		<group name="connection">
			<property name="module" variable="module" type="Int" default="0">
				<allowed name="Satellite" value="0"/>
				<allowed name="Cellular" value="1"/>
			</property>
		</group>
		<group name="registration">
			<property name="status" variable="status" type="Int" default="0">
				<allowed type="value" name="Unknown" value="0"/>
				<allowed type="value" name="Registering" value="1"/>
				<allowed type="value" name="Registered Home Network" value="2"/>
				<allowed type="value" name="Registered Roaming" value="3"/>
				<allowed type="value" name="Registration Denied" value="4"/>
			</property>
		</group>
	</response>	
</cioffi>