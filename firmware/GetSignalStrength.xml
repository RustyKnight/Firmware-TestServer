<cioffi type="function" name="GetSignalStrength">
	<request>
		<header>
			<type>17</type>
			<version>1</version>
		</header>
	</request>
	<response>
		<header>
			<type>18</type>
			<version>1</version>
		</header>
		<group name="cellular">
			<property name="signal" variable="signalStrength" type="Int" default="0">
				<allowed type="range" from="0" to="5"/>
			</property>
		</group>
	</response>	
</cioffi>