<cioffi type="function" name="GetNetworkMode">
	<request>
		<header>
			<type>5</type>
			<version>1</version>
		</header>
	</request>
	<response>
		<header>
			<type>7</type>
			<version>1</version>
		</header>
		<group name="network">
			<property name="mode" variable="mode" type="Int" value="0">
				<allowed type="value" name="Satellite" value="0"/>
				<allowed type="value" name="Cellular" value="1"/>
				<allowed type="value" name="Smart Switch" value="2"/>
			</property>
		</group>
	</response>	
</cioffi>